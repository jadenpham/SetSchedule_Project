<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    @if(ViewBag.data.Count > 0)
    {
        <table>
            <tr>
                <th>Name:</th>
                <th>Address</th>
                <th>Rating</th>
                <th>Status</th>
            </tr>
        @foreach(Data data in ViewBag.data)
        {
            <tr>
                <td><a href="/details/@data.BusinessId" target="_blank">@data.Name</a></td>
                <td>@data.formatted_address</td>
                <td>@data.rating</td>
                <td>@data.Business_Status</td>
            </tr>
        }
        </table>
    }
    else
    {
     <h4>No results</h4>
    }
</body>
</html>